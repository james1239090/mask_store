<div class="row nested-fields form-group">
  <div class="col-md-2">
    <%= f.association :product, label: false, input_html:{class:"select_product", data: {selectfrom: "purchase"}} %>
  </div>
  <div class="col-md-2">
    <%= f.association :color,label: false, input_html:{class:"select_color",data: {selectfrom: "purchase"}} %>
  </div>
  <div class="col-md-1">
    <%= f.association :dimension,label: false, input_html:{class:"select_dimension",data: {selectfrom: "purchase"}} %>
  </div>
  <div class="col-md-2">
    <%= f.input :quantity  ,label: false%>
  </div>
  <div class="col-md-2">
    <%= f.input :currency_price ,label: false %>
  </div>
   <div class="col-md-2">
  <%= f.input :tw_price, label:false %>
  </div>
  <div class="col-md-1"></div>
  <%= link_to_remove_association f, class:"btn" do %>
    <i class="fa fa-trash"></i>
  <% end %>
</div>

