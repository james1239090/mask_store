      <td colspan="10">total_currency_price</td>
      <td><%= @purchase.total_currency_price %></td>
    </tr>
    <tr>
      <td colspan="10">total_tw_price</td>
      <td><%= @purchase.total_tw_price %></td>
    </tr>
    <tr>
      <td colspan="10">total_duty</td>
      <td><%= @purchase.total_tw_duty %></td>
    </tr>
    <tr>
      <td colspan="10">total_service_fee</td>
      <td><%= @purchase.total_tw_service_fee %></td>
    </tr>
    <tr>
      <td colspan="10">purchase_date</td>
      <td><%= @purchase.purchase_date %></td>
    </tr>
    <tr>
      <td colspan="10">delivery_date</td>
      <td><%= @purchase.delivery_date %></td>
    </tr>
