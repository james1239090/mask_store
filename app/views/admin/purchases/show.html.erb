<h2>產品細節</h2>
<table class="table">
  <thead>
    <th>Name</th>
    <th>Color</th>
    <th>Size</th>
    <th>Qty</th>
    <th>Currency Price</th>
    <th>Tw Price</th>
    <th>duty</th>
    <th>shipping</th>
    <th>service fee</th>
    <th>Sub Total</th>
    <th>Total</th>
  </thead>
  <tbody>
    <% @purchase_items.each do |item| %>
      <tr>
        <td><%= item.product.title %></td>
        <td><%= item.color.name %></td>
        <td><%= item.dimension.name %></td>
        <td><%= item.quantity %></td>
        <td><%= item.currency_price %></td>
        <td><%= item.tw_price %></td>
        <td><%= item.duty %></td>
        <td><%= item.shipping_fee %></td>
        <td><%= item.service_fee %></td>
        <td><%= item.sub_total %></td>
        <td><%= item.total %></td>
      </tr>
    <% end %>
    <tr>
      <td><%=  %></td>
    </tr>
  </tbody>
</table>
