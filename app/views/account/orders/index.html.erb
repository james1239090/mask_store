<h2>訂單列表</h2>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>生成時間</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= link_to(order.id,order_path(order.token)) %></td>
        <td><%= render_date_with_format( order.created_at, :long) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
